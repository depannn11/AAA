  "paths": {
  "/ai/unblur": {
      "get": {
        "summary": "AI Photo Unblur (URL)",
        "description": "Menghilangkan blur pada gambar melalui link URL gambar yang sudah ada.",
        "tags": ["AI"],
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "required": true,
            "description": "URL gambar yang ingin di-unblur",
            "schema": { "type": "string", "format": "uri" }
          }
        ],
        "responses": {
          "200": {
            "description": "Berhasil memproses gambar",
            "content": {
              "image/jpeg": {
                "schema": { "type": "string", "format": "binary" }
              }
            }
          }
        }
      }
    },
    "/ai/text2img": {
  "get": {
    "summary": "AI Text to Image",
    "description": "Mengubah teks deskripsi menjadi gambar menggunakan AI.",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "prompt",
        "in": "query",
        "required": true,
        "description": "Deskripsi gambar (English disarankan)",
        "schema": { "type": "string", "example": "Cyberpunk city with neon lights" }
      }
    ],
    "responses": { "200": { "description": "OK" } }
  }
},
    "/ai/heckai": {
  "get": {
    "summary": "Heckai AI Premium (Grok-3 & Llama-4)",
    "description": "Chat dengan model AI terbaru seperti Grok-3 Mini atau Llama-4 Scout melalui Heckai.",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "q",
        "in": "query",
        "required": true,
        "description": "Pertanyaan untuk AI",
        "schema": { "type": "string", "example": "Jelaskan apa itu Grok-3!" }
      },
      {
        "name": "model",
        "in": "query",
        "required": false,
        "description": "Pilihan model: x-ai/grok-3-mini-beta, meta-llama/llama-4-scout",
        "schema": { "type": "string", "default": "x-ai/grok-3-mini-beta" }
      },
      {
        "name": "sessionId",
        "in": "query",
        "required": false,
        "description": "Session ID untuk melanjutkan percakapan (opsional)",
        "schema": { "type": "string" }
      }
    ],
    "responses": {
      "200": { "description": "Berhasil mendapatkan jawaban" }
    }
  }
},
    "/ai/copilot-voice": {
  "get": {
    "summary": "Copilot Voice AI (JSON)",
    "description": "Mendapatkan jawaban AI dalam format teks dan base64 audio.",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "text",
        "in": "query",
        "required": true,
        "description": "Input pertanyaan",
        "schema": { "type": "string", "example": "Halo, apa kabar?" }
      }
    ],
    "responses": {
      "200": { "description": "OK" }
    }
  }
},
    "/ai/zonerai": {
  "get": {
    "summary": "AI Text to Image (Zonerai)",
    "description": "Generate gambar dari teks menggunakan Zonerai AI. Hasil langsung berupa gambar.",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "prompt",
        "in": "query",
        "required": true,
        "description": "Deskripsi gambar yang ingin dibuat",
        "schema": { "type": "string", "example": "cyberpunk samurai girl, ultra detailed, 4k" }
      },
      {
        "name": "size",
        "in": "query",
        "required": false,
        "description": "Pilih ukuran gambar (Dropdown)",
        "schema": {
          "type": "string",
          "default": "1024x1024",
          "enum": [
            "1024x1024",
            "1216x832",
            "1152x896",
            "1344x768",
            "1563x640",
            "832x1216",
            "896x1152",
            "768x1344",
            "640x1536"
          ]
        }
      }
    ],
    "responses": {
      "200": { 
        "description": "Image PNG",
        "content": { "image/png": {} }
      }
    }
  }
},
      "/ai/copilot": {
  "get": {
    "summary": "Copilot Chat",
    "description": "Kirim pertanyaan ke Copilot Microsoft dan dapatkan jawaban beserta referensi/citations",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "message",
        "in": "query",
        "required": true,
        "description": "Pesan atau pertanyaan yang ingin dikirim ke Copilot",
        "schema": { "type": "string" }
      },
      {
        "name": "model",
        "in": "query",
        "required": false,
        "description": "Model Copilot yang digunakan 'think-deeper', 'gpt-5'",
        "schema": { "type": "string", "enum": ["default", "think-deeper", "gpt-5"], "default": "default" }
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mendapatkan jawaban dari Copilot",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "text": { "type": "string", "example": "Berikut adalah berita terbaru dari Indonesia..." },
                "citations": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "title": { "type": "string", "example": "Detik News" },
                      "icon": { "type": "string", "example": "https://example.com/icon.png" },
                      "url": { "type": "string", "format": "uri", "example": "https://www.detik.com/..." }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "400": {
        "description": "Request invalid, message kosong",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Message wajib diisi" }
              }
            }
          }
        }
      },
      "500": {
        "description": "Gagal memproses request",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Gagal memproses request" }
              }
            }
          }
        }
      }
    }
  }
},
  "/ai/venice": {
    "get": {
      "summary": "Venice AI Chat",
      "tags": ["AI"],
      "parameters": [
        { "name": "message", "in": "query", "required": true, "schema": { "type": "string" } }
