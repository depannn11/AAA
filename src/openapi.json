{
  "openapi": "1.0.0",
  "info": {
    "title": "Dinzo Apis",
    "author": "D2:业",
    "version": "v1.0.0",
    "description": "Simple and easy to use API."
  },
  "servers": [
    { "url": "/" }
  ],
  "tags": [
    { "name": "AI" },
    { "name": "Anime" },
    { "name": "Downloader" },
    { "name": "Image" },
    { "name": "Stalk" },
    { "name": "Tools" },
    { "name": "search" },
