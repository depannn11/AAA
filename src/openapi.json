  "paths": {
  "/ai/unblur": {
      "get": {
        "summary": "AI Photo Unblur (URL)",
        "description": "Menghilangkan blur pada gambar melalui link URL gambar yang sudah ada.",
        "tags": ["AI"],
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "required": true,
            "description": "URL gambar yang ingin di-unblur",
            "schema": { "type": "string", "format": "uri" }
          }
        ],
        "responses": {
          "200": {
            "description": "Berhasil memproses gambar",
            "content": {
              "image/jpeg": {
                "schema": { "type": "string", "format": "binary" }
              }
            }
          }
        }
      }
    },
    "/ai/skincolor": {
      "get": {
        "summary": "AI Skin Color Editor",
        "description": "Mengubah warna kulit karakter pada gambar menjadi hitam atau putih menggunakan Gemini AI.",
        "tags": ["AI"],
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "required": true,
            "description": "URL gambar yang ingin diedit",
            "schema": { "type": "string", "format": "uri" }
          },
          {
            "name": "action",
            "in": "query",
            "required": true,
            "description": "Pilih 'hitam' untuk menghitamkan atau 'putih' untuk memutihkan",
            "schema": { "type": "string", "enum": ["hitam", "putih"] }
          }
        ],
        "responses": {
          "200": {
            "description": "Berhasil mengedit gambar",
            "content": {
              "image/png": {
                "schema": { "type": "string", "format": "binary" }
              }
            }
          }
        }
      }
    },
      "/ai/copilot": {
  "get": {
    "summary": "Copilot Chat",
    "description": "Kirim pertanyaan ke Copilot Microsoft dan dapatkan jawaban beserta referensi/citations",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "message",
        "in": "query",
        "required": true,
        "description": "Pesan atau pertanyaan yang ingin dikirim ke Copilot",
        "schema": { "type": "string" }
      },
      {
        "name": "model",
        "in": "query",
        "required": false,
        "description": "Model Copilot yang digunakan 'think-deeper', 'gpt-5'",
        "schema": { "type": "string", "enum": ["default", "think-deeper", "gpt-5"], "default": "default" }
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mendapatkan jawaban dari Copilot",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "text": { "type": "string", "example": "Berikut adalah berita terbaru dari Indonesia..." },
                "citations": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "title": { "type": "string", "example": "Detik News" },
                      "icon": { "type": "string", "example": "https://example.com/icon.png" },
                      "url": { "type": "string", "format": "uri", "example": "https://www.detik.com/..." }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "400": {
        "description": "Request invalid, message kosong",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Message wajib diisi" }
              }
            }
          }
        }
      },
      "500": {
        "description": "Gagal memproses request",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Gagal memproses request" }
              }
            }
          }
        }
      }
    }
  }
},
