    "/download/ytmp4": {
  "get": {
    "summary": "YouTube Video Downloader (HD)",
    "description": "Download video YouTube dengan pilihan resolusi via SaveNow",
    "tags": ["Downloader"],
    "parameters": [
      {
        "name": "url",
        "in": "query",
        "required": true,
        "description": "URL Video YouTube",
        "schema": { "type": "string", "example": "https://www.youtube.com/watch?v=dQw4w9WgXcQ" }
      },
      {
        "name": "quality",
        "in": "query",
        "required": false,
        "description": "Resolusi (360, 720, 1080)",
        "schema": { "type": "string", "example": "720" }
      }
    ],
    "responses": {
      "200": { "description": "Berhasil diproses" }
    }
  }
},
        "/download/ytmp3": {
      "get": {
        "summary": "YouTube MP3 Downloader",
        "description": "Convert video YouTube ke MP3 audio melalui server ssyoutube.",
        "tags": ["Downloader"],
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "required": true,
            "description": "URL video YouTube yang ingin di-download",
            "schema": {
              "type": "string",
              "example": "https://www.youtube.com/watch?v=pQCrpk_BCeA"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Berhasil mendapatkan link MP3",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": { "type": "boolean", "example": true },
                    "creator": { "type": "string", "example": "D2:业" },
                    "result": {
                      "type": "object",
                      "properties": {
                        "title": { "type": "string", "example": "Judul Lagu" },
                        "url": { "type": "string", "format": "uri", "example": "https://cdn.link.mp3" }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": { "description": "URL tidak valid atau parameter kurang" },
          "500": { "description": "Gagal memproses video" }
        }
      }
    },
